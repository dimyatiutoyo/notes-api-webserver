{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "object-curly-spacing": "off",
        "max-len": [
            "error",
            {
                "ignorePattern": "^import [^,]+ from |^export | implements",
                "ignoreComments": true
            }
        ],
    }
}